//Addition of 2 input arrays

#include <iostream>

using namespace std;

int main() 
{
    int n;
    scanf("%d",&n);
    int arr1[n], arr2[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr1[i]);
    }
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr2[i]);
    }
    int sum[n]={0};
    for(int i=0;i<n;i++)
    {
        sum[i] = arr1[i] + arr2[i];
        printf("%d ",sum[i]);
    }
}
